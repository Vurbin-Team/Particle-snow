		"main": [
			"cinematic.snowstorm.CinematicSnowstorm"
		],
		"client": [
			"cinematic.snowstorm.CinematicSnowstormClient"
		],
		"fabric-datagen": [
			"cinematic.snowstorm.CinematicSnowstormDataGenerator"
		],
		"modmenu": [
			"cinematic.snowstorm.modMenu.SnowstormModMenuIntegration"
		]
	},
	"mixins": [
		"cinematic-snowstorm.mixins.json",
